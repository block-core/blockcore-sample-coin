{
    "$schema": "http://json.schemastore.org/template",
    "name": "Blockcore Coin",
    "author": "Blockcore",
    "classifications": ["Blockchain", "Cryptocurrency"],
    "identity": "Blockcore.SampleCoin.CSharp",
    "groupIdentity": "Blockcore.SampleCoin",
    "shortName": "blockcorecoin",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "sourceName": "Blockcore.SampleCoin",
    "preferNameDirectory": true,
    "symbols": {
        "coinTicker": {
          "type": "parameter",
          "description": "Replaces coin ticker symbol e.g. 'BTC'",
          "replaces": "XSC",
          "isRequired": true
        },
        "derivedRename": {
          "type": "derived",
          "description": "Replaces file name and text containing 'SampleCoin'",
          "FileRename": "SampleCoin",
          "replaces": "SampleCoin",
          "valueSource": "name",
          "valueTransform": "AfterLastDot",
          "defaultValue": "CoinName"
        }
     },
    "Forms": {
        "AfterLastDot": {
          "identifier": "replace",
          "pattern": "^.*\\.(?=[^\\.]+$)",
          "replacement": ""
        }
    },
